\n                <Board color={props.playerColor}>\n                    {gameState.boardState.map((r, i) => {\n                        baseColor = baseColor === Color.white ? Color.black : Color.white\n                        return (\n                            <Row key={i}>\n                                {r.map((c, j) => {\n                                    baseColor =\n                                        baseColor === Color.white ? Color.black : Color.white\n                                    return (\n                                        <CellComponent\n                                            key={i.toString().concat(j.toString())}\n                                            cell={c}\n                                            coordinates={{ row: i, column: j }}\n                                            potentialMoves={potentialMoves}\n                                            playerColor={props.playerColor}\n                                            gameState={gameState}\n                                            setGameState={setGameState}\n                                            baseColor={baseColor}\n                                            socket={socket}\n                                        />\n                                    )\n                                })}\n                            </Row>\n                        )\n                    })}\n                </Board>\n            </BoardWrapper>\n            {/* <ShareLink gameId={gameState.gameId} /> */}\n        </GameWrapper>\n    )\n}\n\nfunction Winner(props: { winner: Color }) {\n    return (\n        <WinnerWrapper>\n            <WinnerPawn src={`../chess_icons/pawn_${props.winner}.svg`} />\n            Winner\n        </WinnerWrapper>\n    )\n}\n\nconst WinnerWrapper = styled.div`\n    position: absolute;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    font-size: 50px;\n    font-weight: bold;\n    z-index: 2;\n    font-family: ${props => props.theme.fontFamily};\n`\n\nconst WinnerPawn = styled.img`\n    height: 200px;\n    width: 200px;\n    animation: bubble 1s forwards;\n    animation-name: slides;\n    animation-duration: 2s;\n`\n\nconst BoardWrapper = styled.div`\n    flex: 1;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n`\n\nconst GameWrapper = styled.div`\n    display: flex;\n    flex-direction: column;\n    width: 100vw;\n    height: 100vh;\n`\n\nconst Board = styled.div<{ color: Color }>`\n    overflow: hidden;\n    display: table;\n    table-layout: fixed;\n    border-spacing: 3px;\n    border-collapse: separate;\n    transform: ${props => (props.color === Color.black ? 'rotate(180deg)' : 'none')};\n`\n\nconst Row = styled.div`\n    display: table-row;\n`\n","/Users/karimbenyezza/myProjects/portfolio/chess/client/src/components/GameNotFound.tsx",[],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/moves.ts",[],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/components/ShareLink.tsx",[],["64","65"],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/components/Header.tsx",[],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/components/Cell.tsx",[],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/Sound.ts",[],"/Users/karimbenyezza/myProjects/portfolio/chess/client/src/components/CenteredDiv.tsx",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":8,"column":10,"nodeType":"72","messageId":"73","endLine":8,"endColumn":19},{"ruleId":"66","replacedBy":"74"},{"ruleId":"68","replacedBy":"75"},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],"@typescript-eslint/no-unused-vars","'ShareLink' is defined but never used.","Identifier","unusedVar",["76"],["77"],"no-global-assign","no-unsafe-negation"]